            "args": [
                "--iter=2",
                "--min_ts=5",
                "--max_ts=8",
                "--step_size=1",
                "--output_path=data\\experiments\\all"
            ]
        },
        {
            "name": "Python: minitest.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/minitest.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--api=data\\demo\\importData.csv"
            ]
        }
